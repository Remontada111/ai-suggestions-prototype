{
  "name": "ai-figma-codegen",
  "displayName": "AI Figma Codegen",
  "publisher": "crnolic",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onUri"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ai-figma-codegen.openPanel",
        "title": "AI Figma Codegen: Open Panel"
      }
    ]
  },
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "tsc -p .",
    "build:webview": "vite build --config vite.webview.config.ts",
    "watch:webview": "vite build --watch --config vite.webview.config.ts",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "@types/vscode": "^1.102.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vscode/vsce": "^3.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.5.0",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.83.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "strip-ansi": "^7.1.0",
    "unidiff": "^1.0.4",
    "zustand": "^5.0.6"
  }
}
